{
  "implicitDependencies": ["api-spec"],
  "projectType": "library",
  "root": "packages/api-client-node",
  "sourceRoot": "packages/api-client-node",
  "tags": ["scope:shared"],
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "outputPath": "dist/packages/api-client-node",
        "main": "packages/api-client-node/index.ts",
        "tsConfig": "packages/api-client-node/tsconfig.json"
      }
    },
    "gen-client": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "./generate.sh",
        "cwd": "packages/api-client-node"
      },
      "outputs": ["packages/api-client-node"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["packages/api-client-node/**/*.ts"]
      }
    }
  }
}
