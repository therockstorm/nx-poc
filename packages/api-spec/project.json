{
  "root": "packages/api-spec",
  "sourceRoot": "packages/api-spec/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/api-spec",
        "main": "packages/api-spec/src/index.ts",
        "tsConfig": "packages/api-spec/tsconfig.json",
        "assets": ["packages/api-spec/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/api-spec/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/api-spec"],
      "options": {
        "jestConfig": "packages/api-spec/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
