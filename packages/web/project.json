{
  "root": "packages/web",
  "sourceRoot": "packages/web/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "outputPath": "dist/packages/web",
        "main": "packages/web/src/index.ts",
        "tsConfig": "packages/web/tsconfig.json"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["packages/web/**/*.ts"]
      }
    }
  },
  "tags": ["scope:client"]
}
